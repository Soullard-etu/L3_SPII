package exo2;

public class Vecteur3D {
		int x;
		int y;
		int z;
		
	public Vecteur3D(int x, int y, int z) {
		this.x = x;
		this.y = y;
		this.z = z;
	}

	public Vecteur3D() {
		this.x = 0;
		this.y = 0;
		this.z = 0;
	}
	
	public double Norme() {
		return Math.sqrt(x*x + y*y + z*z);
	}
	

	@Override
	public String toString() {
		return "<" + x + "," + y + "," + z + ">";
	}
		
	

}
